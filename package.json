{
  "name": "firesubz",
  "version": "0.1.0",
  "private": true,
  "metadata": {
    "name": "firesubz",
    "version": "0.1.0",
    "description": "A subscription management application",
    "repository": {
      "type": "git",
      "url": "https://github.com/dambar08/firesubz.git"
    },
    "bugs": {
      "url": "https://github.com/dambar08/firesubz/issues"
    }
  },
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:up": "docker compose up",
    "db:studio": "drizzle-kit studio",
    "seed:users": "dotenv -e .env /home/user/.global_modules/bin/tsx seeds/seed-users.ts",
    "seed:subscriptions": "dotenv -e .env /home/user/.global_modules/bin/tsx seeds/seed-subscriptions.ts",
    "seed:notifications": "dotenv -e .env /home/user/.global_modules/bin/tsx seeds/seed-notifications.ts",
    "seed:demo": "dotenv -e .env /home/user/.global_modules/bin/tsx seeds/demo.ts",
    "dev": "next dev --turbo",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit"
  },
